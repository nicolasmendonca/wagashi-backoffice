{
  "name": "wagashi-backoffice",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:nicolasmendonca/wagashi-backoffice.git",
  "author": "Nicolas Mendonca <nicolasmendonca@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/core",
    "packages/next-app",
    "packages/recipe-editor"
  ],
  "devDependencies": {
    "async": "^3.2.0"
  },
  "scripts": {
      "build": "yarn core:build && yarn recipe-editor:build && yarn next-app:build",
      "core:build": "yarn --cwd packages/core build",
      "recipe-editor:build": "yarn --cwd packages/recipe-editor build",
      "recipe-editor:start": "yarn --cwd packages/recipe-editor start",
      "next-app:build": "yarn --cwd packages/next-app build",
      "next-app:start": "yarn --cwd packages/next-app dev"
  }
}
